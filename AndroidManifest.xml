<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wifinet"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.wifinet.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.example.wifinet.OperateActivity"
            android:label="Operate">
            
        </activity>
        <activity 
            android:name="com.example.wifinet.DialogActivity"
            android:label="Dialog"
            android:theme="@android:style/Theme.Dialog"
            ></activity>
    </application>
<uses-permission 
    android:name="android.permission.INTERNET"
    ></uses-permission>
</manifest>
